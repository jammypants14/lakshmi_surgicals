<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
	
	<record id="prior_deed_no_view_form" model="ir.ui.view">
			<field name="name">prior.deed.no.form</field>
			<field name="model">prior.deed.no</field>
